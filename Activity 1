#include <iostream>
#include <string>
using namespace std;

class Car {
public:
    string brand;
    string model;
    int year;

    void displayInfo() {
        cout << "Car Brand: " << brand << endl;
        cout << "Car Model: " << model << endl;
        cout << "Car Year: " << year << endl;
    }
};

int main() {
    Car myCar;

    myCar.brand = "Wuling";
    myCar.model = "Almaz";
    myCar.year = 2023;

    myCar.displayInfo();

    return 0;
}
